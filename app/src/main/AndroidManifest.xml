<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mediaplayer">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bassdrop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BassnectarPlaylist"
            android:label="@string/bassnectar_playlist"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Gramatik"
            android:label="@string/gramatik"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".GRiZ"
            android:label="@string/griz"
            android:parentActivityName=".MainActivity"/>

    </application>
</manifest>